{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": false,
        "debug": false,
        "modules": false,
        "loose": true,
        "targets": {
          "browsers": [
            "last 2 versions",
            "ie >= 7"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-runtime"
  ]
}